{
  "name": "maplee/maplee",
  "description": "Simple file based php router",
  "keywords": [
    "router",
    "php",
    "file-based"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Antoine Rbl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Maplee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maplee\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7"
  }
}
